{
  "name": "nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "dev:server": "ts-node-dev --files -r tsconfig-paths/register --exit-child --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "start": "ts-node src/shared/infra/http/server.ts",
    "test": "jest",
    "prisma:generate:postgres": "prisma generate --schema=src/shared/infra/prisma/postgres/schema-postgres.prisma",
    "prisma:postgres": "prisma migrate dev --schema=src/shared/infra/prisma/postgres/schema-postgres.prisma",
    "prisma:generate:mongodb": "prisma generate --schema=src/shared/infra/prisma/mongodb/schema-mongodb.prisma",
    "prisma:mongodb": "prisma db push --schema=src/shared/infra/prisma/mongodb/schema-mongodb.prisma",
    "prisma:introspect": "prisma introspect",
    "prisma:seed:postgres": "prisma db seed --schema=src/shared/infra/prisma/postgres/schema-postgres.prisma",
    "prisma:studio": "prisma studio --experimental",
    "prisma:postgres:format": "prisma format --schema=src/shared/infra/prisma/postgres/schema-postgres.prisma"
  },
  "prisma": {
    "schema-postgres": "src/shared/infra/prisma/postgres/schema-postgres.prisma",
    "schema-mongodb": "src/shared/infra/prisma/mongodb/schema-mongodb.prisma",
    "seed": "ts-node-dev --files -r tsconfig-paths/register --exit-child --transpile-only --ignore-watch node_modules src/shared/infra/prisma/postgres/dataDefault/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "aws-sdk": "^2.703.0",
    "bcryptjs": "^2.4.3",
