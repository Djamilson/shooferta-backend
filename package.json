    "@types/mime": "^2.0.3",
    "@types/mongodb": "^4.0.7",
    "@types/multer": "^1.4.3",
    "@types/node": "^17.0.23",
    "@types/nodemailer": "^6.4.4",
    "@types/redis": "^4.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
