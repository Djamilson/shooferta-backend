    "crypto": "^1.0.1",
    "data-fns": "^0.1.8",
    "date-fns": "^2.28.0",
    "dayjs": "^1.10.4",
    "dayjs-plugin-utc": "^0.1.2",
    "dotenv": "^8.2.0",
    "es6-shim": "^0.35.6",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.6",
    "ioredis": "^4.28.4",
    "json-formatter-js": "^2.3.4",
    "jsonwebtoken": "^8.5.1",
