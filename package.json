    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "tsyringe": "^4.6.0",
    "uuidv4": "^6.2.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
